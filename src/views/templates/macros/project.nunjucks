{% macro project(project='') %}
{{this}}
<div class="project">
  <a href="{{ project.link }}">
    <img src="{{ project.image }}" />
    <h2>{{ project.name }}</h2>
    <div class="project__description">
      <p class="description">{{ project.description }}</p>
    </div>
  </a>
</div>
{% endmacro %}
